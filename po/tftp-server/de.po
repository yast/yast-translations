#. Tftp-server dialog caption
#: src/include/tftp-server/dialogs.rb:67 src/include/tftp-server/wizards.rb:43
#: src/include/tftp-server/wizards.rb:78
msgid "TFTP Server Configuration"
msgstr "Konfiguration des TFTP-Servers"

#. dialog help text
#: src/include/tftp-server/dialogs.rb:79
msgid "<p><big><b>Configuring a TFTP Server</b></big></p>"
msgstr "<p><big><b>Konfigurieren eines TFTP-Servers</b></big></p>"

#. dialog help text
#: src/include/tftp-server/dialogs.rb:83
msgid ""
"<p>Use this to enable a server for TFTP (trivial file transfer protocol). "
"The server will be started using xinetd.</p>"
msgstr ""
"<p>Hiermit können Sie einen Server für TFTP (Trivial File Transfer Protocol) "
"aktivieren. Der Server wird mit xinetd gestartet.</p>"

#. enlighten newbies, #102946
#. dialog help text
#: src/include/tftp-server/dialogs.rb:91
msgid "<p>Note that TFTP and FTP are not the same.</p>"
msgstr ""
"<p>Beachten Sie, dass ein Unterschied zwischen TFTP und FTP besteht.</p>"

#. dialog help text
#: src/include/tftp-server/dialogs.rb:96
msgid ""
"<p><b>Boot Image Directory</b>:\n"
"Specify the directory where served files are located. The usual value is\n"
"<tt>/tftpboot</tt>. The directory will be created if it does not exist. \n"
"The server uses this as its root directory (using the <tt>-s</tt> option).</"
"p>\n"
msgstr ""
"<p><b>Verzeichnis für Boot-Image</b>:\n"
"Geben Sie das Verzeichnis an, in dem angebotene Dateien gespeichert werden\n"
"sollen. Als Wert wird in der Regel <tt>/tftpboot</tt> angegeben. Wenn das "
"Verzeichnis\n"
"nicht vorhanden ist, wird es erstellt. Der Server verwendet es als Root-"
"Verzeichnis (mithilfe der Option <tt>-s</tt>).</p>\n"

#. Radio button label, disable TFTP server
#: src/include/tftp-server/dialogs.rb:115
msgid "&Disable"
msgstr "&Deaktivieren"

#. Radio button label, disable TFTP server
#: src/include/tftp-server/dialogs.rb:124
msgid "&Enable"
msgstr "&Aktivieren"

#. Text entry label
#. Directory where served files (usually boot images) reside
#: src/include/tftp-server/dialogs.rb:134
msgid "&Boot Image Directory"
msgstr "Verzeichnis für &Boot-Image"

#. push button label
#. select a directory from the filesystem
#: src/include/tftp-server/dialogs.rb:137
msgid "Bro&wse..."
msgstr "Durch&suchen..."

#. push button label
#. display a log file
#: src/include/tftp-server/dialogs.rb:144
msgid "&View Log"
msgstr "Protokoll &anzeigen"

#. error popup
#: src/include/tftp-server/dialogs.rb:207
msgid ""
"The directory must start with a slash (/)\n"
"and must not contain spaces."
msgstr ""
"Das Verzeichnis muss mit einem Schrägstrich (/) beginnen\n"
"und darf keine Leerzeichen enthalten."

#. progress label
#: src/include/tftp-server/wizards.rb:46
msgid "Initializing..."
msgstr "Initialisierung ..."

#. progress label
#: src/include/tftp-server/wizards.rb:81
msgid "Initializing ..."
msgstr "Initialisierung ..."

#. error popup
#. %1 is a command name (or a comma (, ) separated list of them)
#: src/modules/TftpServer.rb:145
msgid ""
"This module can only use xinetd to set up TFTP.\n"
"However, another program is serving TFTP: %1.\n"
"Exiting.\n"
msgstr ""
"Dieses Modul kann zum Einrichten von TFTP nur xinetd verwenden.\n"
"Jedoch bietet ein anderes Programm TFTP: %1.\n"
"Verlassen.\n"

#. xinetd may be needed for other services so we never turn it
#. off. It will exit anyway if no services are configured.
#. If it is running, restart it.
#: src/modules/TftpServer.rb:240
msgid "Cannot reload service %{name}"
msgstr "Der Dienst %{name} kann nicht neu geladen werden"

#. summary header
#: src/modules/TftpServer.rb:303
msgid "TFTP Server Enabled:"
msgstr "TFTP-Server aktiviert:"

#. summary item: an option is turned on
#: src/modules/TftpServer.rb:305
msgid "Yes"
msgstr "Ja"

