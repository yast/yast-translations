#. push button label
#. select a directory from the filesystem
#: src/include/tftp-server/dialogs.rb:137
msgid "Bro&wse..."
msgstr "&Bladeren..."

#. push button label
#. display a log file
#: src/include/tftp-server/dialogs.rb:144
