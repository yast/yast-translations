#. selection box
#: src/include/dhcp-server/options.rb:453
#: src/include/dhcp-server/options.rb:474
#: src/include/dhcp-server/options.rb:567
#: src/include/dhcp-server/options.rb:780
#: src/include/dhcp-server/options.rb:856
msgid "A&ddresses"
