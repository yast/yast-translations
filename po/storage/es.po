# include/partitioning/custom_part_filesystems.ycp:141
# include/partitioning/custom_part_filesystems.ycp:141
# include/partitioning/custom_part_filesystems.ycp:141
#. help text, richtext format
#: src/lib/y2partitioner/widgets/mkfs_optiondata.rb:277
msgid "Specify the number of file allocation tables. The default is 2."
msgstr ""
"Especifica el número de tablas de asignación de archivos . El valor "
"predeterminado es 2."

# include/partitioning/custom_part_filesystems.ycp:164
# include/partitioning/custom_part_filesystems.ycp:164
# include/partitioning/custom_part_filesystems.ycp:164
