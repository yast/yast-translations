msgstr "Alle entfernen"

#. error popup text
#: src/modules/DualMultiSelectionBox.rb:183
msgid "File %1 is not a regular file!"
msgstr "Die Datei '%1' ist keine reguläre Datei!"

#. error popup text
#: src/modules/DualMultiSelectionBox.rb:188
