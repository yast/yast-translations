#: src/modules/DualMultiSelectionBox.rb:134
msgid "Add"
msgstr "Hinzufügen"

#. push button text
#: src/modules/DualMultiSelectionBox.rb:140
msgid "Add All"
msgstr "Alle hinzufügen"

#. push button text
