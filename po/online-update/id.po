msgid "No details available."
msgstr "Tidak ada rincian yang tersedia."

#: src/modules/OnlineUpdateDialogs.rb:45
msgid "&Details <<"
msgstr "&Rincian <<"

#: src/modules/OnlineUpdateDialogs.rb:46
