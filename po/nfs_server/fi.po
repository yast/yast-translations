msgstr "&Jaettava kansio"

#. button label
#: src/include/nfs_server/ui.rb:64
msgid "&Browse..."
msgstr "&Selaa..."

#: src/include/nfs_server/ui.rb:87
msgid "Enter a non-empty export path. For example, /exports."
msgstr "Aseta jakopolku, joka ei ole tyhjä, esimerkiksi /exports."

