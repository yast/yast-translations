#. CLI action handler.
#. @param [Hash] options command options
#. @return whether successful
#: src/clients/nfs_server.rb:340
msgid ""
"Domain cannot be set without enabling NFSv4. Use the 'set enablev4=yes' "
"command."
msgstr ""
"Domain tidak dapat diatur tanpa mengaktifkan NFSv4. Gunakan perintah 'set "
"enablev4=yes'."

#: src/clients/nfs_server.rb:351
msgid ""
"Command 'set' must be used in the form 'set option=value'. Use 'set help' to "
"get information about available options."
msgstr ""
"Perintah 'set' harus digunakan dalam form 'set option=value'. Gunakan 'set "
"help' untuk mendapatkan informasi tentang opsi yang tersedia."

#. Then no need to check for conflict.
#: src/include/nfs_server/routines.rb:205
#: src/include/nfs_server/routines.rb:215
#, ycp-format
msgid ""
"%3 and %4 are both exported with the option fsid=0\n"
"for the same client '%1' (contained in '%2')"
msgstr ""
"%3 dan %4 keduanya diekspor dengan opsi fsid=0\n"
"untuk klien yang sama '%1' (terkandung dalam '%2')"

#. selection box label
#: src/include/nfs_server/routines.rb:313
msgid "Dire&ctories"
msgstr "Dire&ktori"

#. error popup message
#: src/include/nfs_server/routines.rb:331
msgid ""
"The wild card or options string is invalid.\n"
"It must be shorter than 70 characters and it\n"
"must not contain spaces.\n"
msgstr ""
"String wild card atau opsi tidak valid.\n"
"Ini harus lebih pendek dari 70 karakter dan\n"
"tidak boleh berisi spasi.\n"

#. error popup message
#: src/include/nfs_server/routines.rb:353
msgid ""
"Invalid option.\n"
"Only letters, digits, and the characters =/.:,_- are allowed."
msgstr ""
"Opsi tidak valid.\n"
"Hanya huruf, digit, dan karakter =/.:,_- yang diizinkan."

#. error popup message
#: src/include/nfs_server/routines.rb:401
#, ycp-format
