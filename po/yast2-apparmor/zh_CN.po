#. [ never_ask_again     - string true/false - add widget to let ]
#. [                       user select to never prompt again to  ]
#. [                       upload unselected profiles to the     ]
#. [                       repository                            ]
#. [ default_select      - string true/false - default value for ]
#. [                       profile selection                     ]
