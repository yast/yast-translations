#. @param [Hash] agent_data - data from the backend
#. [ repo_url - string ]
#. @return answers - map that contains:
#. [   newuser     => 1|0 - registering a new user?         ]
#. [   user        => username                              ]
#. [   pass        => password                              ]
#. [   email       => email address - if newuser = 1        ]
#. [   save_config => true/false - save this information on ]
#. [                  the system                            ]
