#. [ PROFILES         - list[string] - list of selected profiles ]
#. [ NEVER_ASK_AGAIN  - string - true/false - mark unselected    ]
#. [                    profiles as local only and don't prompt  ]
#. [                    to upload                                ]
#. [ CHANGELOG        - map[string,string] - changelog data from ]
#. [                    UI_ChangeLog_Dialog()                    ]
#.
