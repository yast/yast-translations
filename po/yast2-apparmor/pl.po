#~ "        Zmienne to:\n"
#~ "        <ul> \n"
#~ "        <li>GCONV_PATH</li>\n"
#~ "        <li>GETCONF_DIR</li>\n"
#~ "        <li>HOSTALIASES</li>\n"
#~ "        <li>LD_AUDIT</li>\n"
#~ "        <li>LD_DEBUG</li>\n"
#~ "        <li>LD_DEBUG_OUTPUT</li>\n"
