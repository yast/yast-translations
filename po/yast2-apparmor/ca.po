#: src/include/apparmor/profile_dialogs.rb:153
#: src/include/apparmor/profile_dialogs.rb:179
msgid "All"
msgstr "Tot"

#: src/include/apparmor/profile_dialogs.rb:199
msgid "Network Family"
msgstr "Família de xarxa"

