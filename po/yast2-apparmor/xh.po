#~ "         <li>i-HOSTALIASES</li>\n"
#~ "         <li>UKUPHICOTHA_i-LD</li>\n"
#~ "         <li>iLD_DEBUG</li>\n"
#~ "         <li>i-LD_DEBUG_OUTPUT</li>\n"
#~ "         <li>LD_DYNAMIC_WEAK</li>\n"
#~ "         <li>INDLELA_YETHALA_le-LD</li>\n"
#~ "         <li>INDLELA_ye-LD_ENGUNDOQO</li>\n"
#~ "         <li>i-LD_FAKA KWANGAPHAMBILI</li>\n"
#~ "         <li>IPROFAYILI_ye-LD</li>\n"
#~ "         <li>LD_SHOW_AUXV</li>\n"
#~ "         <li>i-LD_ISEBENZISA_i-BIAS_YOKUFAKA</li>\n"
#~ "         <li>LOCALDOMAIN</li>\n"
#~ "         <li>INDLELA YOKUTSHIXA</li>\n"
#~ "         <li>MALLOC_TRACE</li>\n"
#~ "         <li>INDLELA ye-NLS</li>\n"
#~ "         <li>GQIBA_UBUMBEKO_YOMAMKELI</li>\n"
#~ "         <li>i-RES_ENOKUKHETHWA</li>\n"
#~ "         <li>i-TMPDIR</li>\n"
#~ "         <li>i-TZDIR</li> </ul>\n"

