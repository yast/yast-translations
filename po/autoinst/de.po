msgid "No control files defined"
msgstr "Keine Steuerdateien definiert"

#: src/include/autoinstall/classes.rb:443
msgid "&Merge with Empty Control File"
msgstr "Mit leerer Steuerdatei &zusammenfügen"

#: src/include/autoinstall/classes.rb:450
