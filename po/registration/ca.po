#. using media is not supported
#: src/lib/registration/ui/migration_repos_workflow.rb:810
msgid ""
"<h2>Warning!</h2><p><b>The media based upgrade for registered systems is not "
"supported!<b></p>"
msgstr ""
"<h2>Avís!</h2><p><b>No s'admet l'actualització basada en mitjans per a "
"sistemes registrats!<b></p>"

#: src/lib/registration/ui/migration_repos_workflow.rb:812
