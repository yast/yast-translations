msgstr "Um problema ocorreu durante a conexão ao servidor LDAP."

#. error message, more specific description follows
#: src/Ldap.rb:553
msgid "A problem occurred while reading data from the LDAP server."
msgstr "Um problema ocorreu durante a leitura de dados do servidor LDAP."

#. error message, more specific description follows
#: src/Ldap.rb:557
msgid "A problem occurred while writing LDAP users."
msgstr "Um problema ocorreu durante a escrita dos usuários LDAP."

#. error message, more specific description follows
#: src/Ldap.rb:561
msgid "A problem occurred while writing LDAP groups."
msgstr "Um problema ocorreu durante a escrita dos grupos LDAP."

#. error message, more specific description follows
#: src/Ldap.rb:565
