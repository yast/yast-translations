#~ "         والمتغيرات هي:         <ul> \n"
#~ "         <li>GCONV_PATH</li>\n"
#~ "         <li>GETCONF_DIR</li>\n"
#~ "         <li>HOSTALIASES</li>\n"
#~ "         <li>LD_AUDIT</li>\n"
#~ "         <li>LD_DEBUG</li>\n"
#~ "         <li>LD_DEBUG_OUTPUT</li>\n"
#~ "         <li>LD_DYNAMIC_WEAK</li>\n"
#~ "         <li>LD_LIBRARY_PATH</li>\n"
#~ "         <li>LD_ORIGIN_PATH</li>\n"
#~ "         <li>LD_PRELOAD</li>\n"
#~ "         <li>LD_PROFILE</li>\n"
#~ "         <li>LD_SHOW_AUXV</li>\n"
#~ "         <li>LD_USE_LOAD_BIAS</li>\n"
#~ "         <li>LOCALDOMAIN</li>\n"
#~ "         <li>LOCPATH</li>\n"
#~ "         <li>MALLOC_TRACE</li>\n"
#~ "         <li>NLSPATH</li>\n"
#~ "         <li>RESOLV_HOST_CONF</li>\n"
#~ "         <li>RES_OPTION</li>\n"
