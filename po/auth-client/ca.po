#. Enable and start the service.
#: src/lib/auth/authconf.rb:157
#, c-format
msgid ""
"Failed to start service %s. Please use system journal (journalctl -n -u %s) "
"to diagnose."
msgstr ""
"Ha fallat iniciar el servei %s. Si us plau, useu el diari del sistema per a "
"la diagnosi (journalctl -n -u %s)."

#. Disable and stop the service.
#: src/lib/auth/authconf.rb:165
#, c-format
msgid ""
"Failed to stop service %s. Please use system journal (journalctl -n -u %s) "
"to diagnose."
msgstr ""
"Ha fallat aturar el servei %s. Si us plau, useu el diari del sistema per a "
"la diagnosi (journalctl -n -u %s)."

#. Only install the required provider packages. By convention, they are named 'sssd-*'.
#: src/lib/auth/authconf.rb:410
msgid "Failed to install software packages required for running SSSD."
msgstr ""
"Ha fallat instal·lar els paquets de programari necessaris per executar "
"l'SSSD."

#. Install required packages
#: src/lib/auth/authconf.rb:731
#, fuzzy
#| msgid "Failed to install software packages required by autofs/nscd daemons."
msgid "Failed to install software packages required by autofs daemon."
msgstr ""
"Ha fallat instal·lar els paquets de programari necessaris per als dimonis "
"autofs/nscd."

#. Create a temporary file holding smb.conf for the specified AD domain.
#. @return [File] a closed file, caller should #unlink after it is no longer used.
#: src/lib/auth/authconf.rb:788
msgid "Failed to check the installed samba version."
msgstr "No s'ha pogut comprovar la versió de samba instal·lada."

#. Call package installer to install Samba and Kerberos if it has not yet been installed.
#: src/lib/auth/authconf.rb:824
msgid ""
"Failed to install Samba & Kerberos required by Active Directory operations."
msgstr ""
"Ha fallat instal·lar Samba i Kerberos, necessaris per a operacions d'Active "
"Directory."

#. Run "net ads join". Return tuple of boolean success status and command output.
#. Kerberos configuration must have been read before calling this function.
#. Kerberos configuration will be written.
#: src/lib/auth/authconf.rb:898
msgid "Nothing is done because AD is not configured"
msgstr "No es fa res perquè AD no està configurat."

#: src/lib/auth/authconf.rb:901
msgid "Failed to install Samba"
msgstr "Ha fallat instal·lar Samba."

#. Configure Kerberos
#: src/lib/auth/authconf.rb:906
msgid ""
"Cannot locate Active Directory's Kerberos via DNS lookup.\n"
"Please configure your network environment to use AD server as the name "
"resolver."
msgstr ""
"No es pot ubicar el Kerberos d'Active Directory a través de la cerca DNS.\n"
"Si us plau, configureu l'entorn de xarxa per utilitzar el servidor d'AD com "
"a resolutor de noms."

#. Create a temporary smb.conf to join this computer
#: src/lib/auth/authconf.rb:915
msgid "Failed to create temporary smb.conf"
msgstr "No s'ha pogut crear smb.conf temporal."

