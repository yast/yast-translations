msgid "&MD5"
msgstr "&MD5"

#. Radio buttons for password encryption: sha256 crypt
#: src/include/users/widgets.rb:520
msgid "SHA-&256"
msgstr "SHA-&256"

#. Radio buttons for password encryption: sha512 crypt
#: src/include/users/widgets.rb:528
