#: src/include/firewall/subdialogs.rb:87
msgid "Firewall Interfaces"
msgstr "การใช้ไฟร์วอลล์กับส่วนเชื่อมต่อ"

#. TRANSLATORS: table header item
#: src/include/firewall/subdialogs.rb:95
msgid "Device"
