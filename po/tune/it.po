#. Command line help text for the hardware detection module, %1 is "hwinfo"
#: src/clients/hwinfo.rb:49
#, ycp-format
msgid ""
"Hardware Detection - this module does not support the command line "
"interface, use '%1' instead."
msgstr ""
"Rilevamento Hardware - questo modulo non supporta l'interfaccia a riga di "
"comando. Usare %1."

#. translators: popup heading
#: src/clients/hwinfo.rb:73
msgid "Probing Hardware..."
msgstr "Rilevamento dell'hardware in corso…"

#. progress bar label
#: src/clients/hwinfo.rb:76
msgid "Progress"
msgstr "Avanzamento"

#. title label
