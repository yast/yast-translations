msgstr "ਕੀ-ਬੋਰਡ ਲੇਆਉਟ(&K)"

#. help text for firstboot language + keyboard screen
#: src/clients/firstboot_language_keyboard.rb:113
#, fuzzy
#| msgid ""
#| "<p>\n"
#| "Choose the <b>Language</b> and the <b>Keyboard layout</b> to be used "
#| "during\n"
