msgid "First Boot Configuration"
msgstr "Ensimmäisen käynnistyksen määritys"

#: src/clients/firstboot_config.rb:59
msgid "&Up"
msgstr "&Ylös"

#: src/clients/firstboot_config.rb:60
msgid "D&own"
msgstr "&Alas"

#: src/clients/firstboot_config.rb:61
msgid "Enab&le or Disable"
